{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "deploy": {
      "cache": false
    },
    "destroy": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "testu": {
      "cache": false
    },
    "test:integ": {
      "cache": false
    }
  }
}
